{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    //"umbracoDbDSN": "Data Source=|DataDirectory|/UmbracoSamples.sqlite;Cache=Shared;Foreign Keys=True;Pooling=True",
    //"umbracoDbDSN_ProviderName": "Microsoft.Data.SQLite"
    "umbracoDbDSN": "server=(localdb)\\MSSQLLocalDB;database=Umbraco.Docs.Samples;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\\Umbraco.Docs.Samples.mdf"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "524ab247-0239-41e3-93c4-b6a9445cbbf7"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      }
    }
  }
}